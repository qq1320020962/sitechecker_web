<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="image-default" extends="struts-default" namespace="">
	
		<action name="imageAction_*" class="imageAction" method="{1}">
			<result name="success">/WEB-INF/webadmin/dosuccess.jsp</result>
			<result name="addImgUI">WEB-INF/webadmin/addpicture.jsp</result>
			<result name="imgList">WEB-INF/webadmin/imglist.jsp</result>
			<result name="action2action" type="redirectAction">imageAction_imgList</result>
		</action>
	</package>
</struts>